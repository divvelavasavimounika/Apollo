<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>Application properties in XML format</comment>

    <entry key="server.port">8085</entry>
    <entry key="server.servlet.contextPath">/irmapi</entry>

    <entry key="payout.lowerBoundAmount">500000</entry>
    <entry key="payout.upperBoundAmount">700000</entry>
    
    <entry key="spring.datasource.url">jdbc:h2:file:./data/h2.db</entry>
    <entry key="spring.h2.console.enabled">true</entry>
    <entry key="spring.h2.console.path">/h2</entry>
    <entry key="spring.datasource.username">sa</entry>
    <entry key="spring.datasource.password"></entry>
    <entry key="spring.datasource.driverClassName">org.h2.Driver</entry>
    <entry key="spring.jpa.database-platform">org.hibernate.dialect.H2Dialect</entry>

    
</properties>